# timescaledb extension
comment = 'Enables scalable inserts and complex queries for time-series data (Community Edition)'
default_version = '2.16.1'
module_pathname = '$libdir/timescaledb-2.16.1'
#extension cannot be relocatable once installed because it uses multiple schemas and that is forbidden by PG.
#(though this extension is relocatable during installation).
relocatable = false
trusted = true
